#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter the three numbers:-");
    scanf("%d%d%d",&a,&b,&c);
    if (a>b && a>c) 
    { if (b<a && b>c)
        {printf("\n%d<%d<%d",c,b,a);}
        if (c<a && c>b)
        {printf("\n%d<%d<%d",b,c,a);}
    }
   if (b>a && b>c) 
    { if (a<b && a>c)
        {printf("\n%d<%d<%d",c,a,b);}
        if (c<b && c>a)
        {printf("\n%d<%d<%d",a,c,b);}
    }
    if (c>a && c>b) 
    { if (c>b && b>a)
        {printf("\n%<%d<%d",a,b,c);}
        if (b<c && b<a)
        {printf("\n%d<%d<%d",b,a,c);}
    }
    else if(a=b=c){printf("all the numbers are same");}

return 0;
}
